# AI Voice Agent SaaS - Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://user:password@host:port/database_name
ANALYTICS_DATABASE_URL=postgresql://user:password@host:port/analytics_db
TIMESERIES_DATABASE_URL=http://localhost:8086

# SSH Configuration (for VPS management)
SSH_HOST=your.server.ip.address
SSH_USER=your_ssh_username
SSH_KEY_PATH=./path/to/your/ssh/key

# GitHub Integration
GITHUB_TOKEN=ghp_your_github_personal_access_token

# Notion Integration (for documentation/notes)
NOTION_API_KEY=ntn_your_notion_integration_token

# PayPal Configuration
PAYPAL_CLIENT_ID=your_paypal_app_client_id
PAYPAL_CLIENT_SECRET=your_paypal_app_client_secret
PAYPAL_ENVIRONMENT=sandbox
PAYPAL_WEBHOOK_ID=your_paypal_webhook_id

# Twilio Configuration (for phone numbers and SMS)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number
TWILIO_WEBHOOK_URL=https://yourdomain.com/webhooks/twilio

# AI Voice Agent Configuration
VOICE_AGENT_API_KEY=your_voice_agent_api_key
OPENAI_API_KEY=your_openai_api_key
DEEPGRAM_API_KEY=your_deepgram_api_key
CARTESIA_API_KEY=your_cartesia_api_key

# LiveKit Configuration (for voice processing)
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
LIVEKIT_URL=wss://your-livekit-server.com

# Ollama Configuration (for local LLM)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2:3b

# Voice Processing Configuration
STT_PROVIDER=deepgram
TTS_PROVIDER=cartesia
VOICE_CLONING_PROVIDER=elevenlabs

# Voice Cloning Services
ELEVENLABS_API_KEY=your_elevenlabs_api_key
VOICE_CLONING_API_KEY=your_voice_cloning_api_key

# Infrastructure Configuration
CONTAINER_REGISTRY_URL=registry.yourdomain.com
DOCKER_HOST=unix:///var/run/docker.sock
DEPLOYMENT_ENV=production

# Client Onboarding Configuration
DOMAIN_BASE=yourdomain.com
SSL_CERT_EMAIL=admin@yourdomain.com

# Email Configuration (for notifications)
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_SMTP_USER=your_email@gmail.com
EMAIL_SMTP_PASS=your_app_password
FROM_EMAIL=noreply@yourdomain.com

# Analytics and Monitoring
DASHBOARD_URL=https://grafana.yourdomain.com
ALERT_EMAIL=alerts@yourdomain.com
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url
RETENTION_DAYS=365

# Monitoring Services
PROMETHEUS_URL=http://localhost:9090
GRAFANA_URL=http://localhost:3000
GRAFANA_API_KEY=your_grafana_api_key

# Application Configuration
APP_ENV=development
APP_PORT=3000
APP_DOMAIN=localhost:3000

# Report Storage
REPORT_STORAGE_PATH=./reports